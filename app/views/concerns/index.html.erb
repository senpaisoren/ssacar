<p id="notice"><%= notice %></p>

<h1>Concerns</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Texttype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concerns.each do |concern| %>
      <tr>
        <td><%= concern.title %></td>
        <td><%= concern.content %></td>
        <td><%= concern.texttype %></td>
        <td><%= link_to 'Show', concern %></td>
        <td><%= link_to 'Edit', edit_concern_path(concern) %></td>
        <td><%= link_to 'Destroy', concern, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Concern', new_concern_path %>
